&i2c15
{       /*APPS_I2C */
        status = "okay";
        clock-frequency  = <400000>;

        adlbmc:adl-bmc@28 {
                compatible = "adl-bmc";
                reg=<0x28>;
                #address-cells = <1>;
                #size-cells = <0>;
                status = "okay";
        };

        pcf8563: pcf8563@51 {
                compatible = "nxp,pcf8563";
                reg = <0x51>;
                #clock-cells = <0>;
                status = "okay";
        };
        /*       TPM - ST33TPHF2XI2C */

        tpm:  st33tphf2xi2c@2e {
                #gpio-cells = <2>;
                #interrupt-cells = <2>;
//                compatible = "st,st33htpm-i2c";
                compatible = "infineon,slb9673";
                reg = <0x2e>;

                label = "tpm";
                pinctrl-names = "default";
                pinctrl-0 = <&nfc_int_active &nfc_enable_active &dsi0_lcd0_en &dsi0_TE_en>; //gpio110 - TPM_PP ; gpio111 - TPM_LP
                interrupt-parent = <&tlmm>;
                interrupts = <88 0>;            //TPM_IRQ_N
                reset-gpio = <&tlmm 92 0>;      //TPM_RST_N
                status = "okay";

                };

};

&tlmm{

  nfc_enable_active: nfc_enable_active {
                           /* active state */
                           mux {
                                   /* 6: Enable 110: Firmware */
                                   pins = "gpio110";
                                   function = "gpio";
                           };

                           config {
                                   pins = "gpio110";
                                   drive-strength = <2>; /* 2 MA */
                                   bias-pull-up;
                           };
                   };
        nfc_int_active: nfc_int_active {
                        /* active state */
                        mux {
                                /* GPIO 111 NFC Read Interrupt */
                                pins = "gpio111";
                                function = "gpio";
                        };

                        config {
                                pins = "gpio111";
                                drive-strength = <2>; /* 2 MA */
                                bias-pull-up;
                        };
                };
	dsi0_lcd0_en: dsi0_lcd0_en{
                      mux {
                              pins =  "gpio73";
                              function = "gpio";
                      };

                      config {
                              pins = "gpio73";
                              bias-pull-down;
                              drive-strength = <4>;
                              output-high;
                              input-disable;
                              power-source = <2>;
                      };
              };

        dsi0_back_light_en: dsi0_back_light_en{
                            mux {
                                    pins =  "gpio72";
                                    function = "gpio";
                            };

                            config {
                                    pins = "gpio72";
                                    bias-pull-down;
                                    drive-strength = <4>;
                                    output-high;
                                    input-disable;
                                    power-source = <2>;
                            };
                    };

        dsi0_TE_en: dsi0_TE_en{
                            mux {
                                    pins =  "gpio66";
                                    function = "gpio";
                            };

                            config {
                                    pins = "gpio66";
                                    bias-pull-down;
                                    drive-strength = <4>;
                                    output-high;
                                    input-disable;
                                    power-source = <2>;
                            };
                    };



};

