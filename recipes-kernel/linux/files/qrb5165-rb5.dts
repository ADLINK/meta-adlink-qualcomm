&cdsp {
	status = "okay";
	firmware-name = "qcom/sm8250/cdsp.mbn";
};

&dsi0 {
	status = "okay";
	vdda-supply = <&vreg_l9a_1p2>;

#if 0
	qcom,dual-dsi-mode;
	qcom,master-dsi;
#endif
	ports {
		port@1 {
			endpoint {
				remote-endpoint = <&simple_lt96>;
				data-lanes = <0 1 2 3>;
			};
		};
	};
	        panel@0 {
                compatible = "adlink,lt9611uxc";
                reg = <0>;
                status = "ok";
                ports {
                        #address-cells = <1>;
                        #size-cells = <0>;

                        port@0 {
                                reg = <0>;
                                simple_lt96: endpoint {
                                        remote-endpoint = <&lt9611_a>;
                                };
                        };
                };

           };

};

&dsi0_phy {
	status = "okay";
	vdds-supply = <&vreg_l5a_0p88>;
};

&gmu {
	status = "okay";
};

&gpu {
	status = "okay";

	zap-shader {
		memory-region = <&gpu_mem>;
		firmware-name = "qcom/sm8250/a650_zap.mbn";
	};
};

/* LS-I2C0 */
&i2c4 {
        clock-frequency  = <400000>;
	status = "disabled";
};

&i2c5 {
	status = "okay";
	clock-frequency = <400000>;

	lt9611_codec: hdmi-bridge@2b {
		compatible = "lontium,lt9611uxc";
		reg = <0x2b>;
		#sound-dai-cells = <1>;

		interrupts-extended = <&tlmm 63 IRQ_TYPE_EDGE_FALLING>;

		reset-gpios = <&pm8150l_gpios 5 GPIO_ACTIVE_HIGH>;

