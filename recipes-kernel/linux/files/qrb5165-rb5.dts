	vdda-phy-supply = <&vreg_l5a_0p88>;
	vdda-pll-supply = <&vreg_l9a_1p2>;

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		/* The port index denotes CSIPHY id i.e. csiphy2 */
		port@0 {
			reg = <0>;
			csiphy0_ep: endpoint {
				clock-lanes = <0>;
				data-lanes = <1 2>;
				remote-endpoint = <&imx219_ep0>;
			};

		};
		port@2 {
			reg = <2>;
			csiphy2_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&imx219_ep2>;
			};

		};
		port@3 {
			reg = <3>;
			csiphy3_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&imx219_ep3>;
			};

		};
		port@1 {
			reg = <1>;
			csiphy1_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&imx219_ep1>;
			};

		};
		port@4 {
			reg = <4>;
			csiphy4_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&imx219_ep4>;
			};

		};
		port@5 {
			reg = <5>;
			csiphy5_ep: endpoint {
				clock-lanes = <7>;
				data-lanes = <0 1 2 3>;
				remote-endpoint = <&imx219_ep5>;
			};

		};
	};
};

&cci0 {
	clock-frequency=<37500000>;
	status = "okay";
};

&cci0_i2c0 {
	//CI_I2C_SCL0 - CCI_I2C_SDA0 		

	ov5640@3c {
		 // CAM_0 
//		compatible = "sony,imx219";
		compatible = "ovti,ov5640";
		reg = <0x3c>;

		reset-gpios = <&tlmm 3 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default", "suspend";

	        pinctrl-0 = <&cam_sensor_mclk0_active
                                &cam_sensor0_power_adlink_active
                                &cam_sensor0_active_adlink_rst0 >;
                pinctrl-1 = <&cam_sensor_mclk0_suspend
                                &cam_sensor0_power_adlink_suspend
                                &cam_sensor0_suspend_adlink_rst0>;

		clocks = <&camcc CAM_CC_MCLK0_CLK>;
		assigned-clocks = <&camcc CAM_CC_MCLK0_CLK>;
		assigned-clock-rates = <24000000>;

		power-domains = <&camcc TITAN_TOP_GDSC>;
		DOVDD-supply  = <&vreg_l7f_1p8>;
		AVDD-supply = <&vreg_l6f_2p8>;
		DVDD-supply = <&vreg_l2f_1p2>;
	
//		dovdd-supply  = <&vreg_l7f_1p8>;
//		avdd-supply = <&vdc_5v>;
//		dvdd-supply = <&vdc_5v>;

		status = "okay";
		port {
			imx219_ep0: endpoint {
				clock-lanes = <0>;
				link-frequencies = /bits/ 64 <456000000>;
				data-lanes = <1 2>;
				remote-endpoint = <&csiphy0_ep>;
			};
		};
	};
	// CAM_2 
	imx214 {
		compatible = "sony,imx214";
		reg = <0x1a>;

		reset-gpios = <&tlmm 116 GPIO_ACTIVE_LOW>;
		pinctrl-names = "default", "suspend";

                pinctrl-0 = <&cam_sensor_mclk2_active
                                 &cam_sensor_active_adlink_rst2
                                 &cam_sensor_power_adlink_active>;
                pinctrl-1 = <&cam_sensor_mclk2_suspend
                                 &cam_sensor_suspend_adlink_rst2
                                 &cam_sensor_power_adlink_suspend>;

		clocks = <&camcc CAM_CC_MCLK2_CLK>;
		assigned-clocks = <&camcc CAM_CC_MCLK2_CLK>;
		assigned-clock-rates = <24000000>;

		power-domains = <&camcc TITAN_TOP_GDSC>;
		dovdd-supply  = <&vreg_l7f_1p8>;
		avdd-supply = <&vdc_5v>;
		dvdd-supply = <&vdc_5v>;

		status = "disabled";
		port {
			imx219_ep2: endpoint {
				clock-lanes = <1>;
				link-frequencies = /bits/ 64 <600000000>;
				data-lanes = <1 2 3 4>;
				remote-endpoint = <&csiphy2_ep>;
			};
		};
	};
};

